 <h1><%= @current_user.username%></h1>

<dl>
  <dt>Private</dt>
  <dd>false</dd>

  <dt>Followers</dt>
  <dd>
    0
  </dd>

  <dt>Following</dt>
  <dd>0</dd>
<nav>
  <ul>
    <li>
      <a href="/users/<%= @current_user.username%>">Profile</a>
    </li>
    <li>
      <a href="/users/<%= @current_user.username%>/liked_photos">Liked photos</a>
    </li>
    <li>
      <a href="/users/<%= @current_user.username%>/feed">Feed</a>
    </li>
    <li>
      <a href="/users/<%= @current_user.username%>/discover">Discover</a>
    </li>
  </ul>
</nav>
</dl>
<% if @list_of_likes != nil %>
<h2>Liked photos (<%= @list_of_likes.count %>)</h2>


<table border="1">
  <tr>
    <th>
      Image
    </th>

    <th>
      Owner
    </th>

    <th>
      Caption
    </th>

    <th>
      Posted
    </th>

    <th>
      Likes
    </th>

    <th></th>
  </tr>


<% @list_of_likes.each do |a_like| %>
      <tr>
        <td>
        <img src="<%= a_like.photo.image %>">
        </td>

        <td>
        <%= a_like.photo.poster.username %>

        <td>
        <%= a_like.photo.caption %> 
        </td>

        <td>
        <%= a_like.photo.created_at %>
        </td>

        <td>
        <%= a_like.photo.likes_count %>      
        </td>

        <td>
          <a href="/photos/<%= a_like.photo.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
      <% end %>
</table>
